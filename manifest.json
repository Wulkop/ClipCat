{
  "manifest_version": "1",
  "type": "WebApp",
  "meta": {
    "name": "ClipCat",
    "version": "0.0.1",
    "minimum-overwolf-version": "0.120.1.0",
    "author": "Wully0x7CD",
    "icon": "icon.png",
    "icon_gray": "icon_gray.png",
    "description": "An overwolf in-game overlay to categorize your best gaming clips directly after recording."
  },
  "permissions": [
    "Hotkeys",
    "Extensions",
    "Streaming",
    "Profile",
    "GameInfo",
    "DesktopStreaming",
    "FileSystem"

  ],
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        10826
      ]
    },
    "start_window": "index",
    "windows": {
      "index": {
        "file": "index.html",
        "transparent": true,
        "clickthrough": false,
        "resizable": false,
        "show_in_taskbar": true,
        "size": {
          "width": 200,
          "height": 500
        },
        "start_position": {
          "Top": 0,
          "Left": 0
        }
      }
    },
    "hotkeys": {
      "sample_app_showhide": {
        "title": "Categorize last clip",
        "action-type": "custom",
        "default": "F1"
      }
    },
    "extra-objects": {
      "simple-io-plugin": {
        "file": "simple-io-plugin.dll",
        "class": "overwolf.plugins.simpleio.SimpleIOPlugin"
      }
    },
    "game_events": [10826],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            10826
          ]
        },
        "start_minimized": false
      }]
    
  }
}
